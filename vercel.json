{
    "version" : 1,
    "builds": [
        {
            "src": "src/node/validator.js",
            "use": "@vercel/node"
        },
        {
            "src": "src/app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/node/validator.js"
        },
        {
            "src": "/(.*)",
            "dest": "src/app.py"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "src/app.py"
        }
    ]
}
