{
    "version" : 2,
    "builds": [
        {
            "src": "src/app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/app.py"
        }
    ]
}

{
    "version": 2,
    "builds": [
      {
        "src": "backend/api/**",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/$1"
      }
    ]
  }
